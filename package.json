{
  "name": "node-mikro-orm-playground",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "export NODE_ENV=development && bun ./src/app.ts",
    "start": "export NODE_ENV=production && bun ./dist/src/app.js",
    "migration:create": "bun run ./scripts/mikro-orm/migrationCreate.ts",
    "migration:up": "bun run ./scripts/mikro-orm/migrationUp.ts"
  },
  "dependencies": {
    "@mikro-orm/cli": "5.8.6",
    "@mikro-orm/core": "5.8.6",
    "@mikro-orm/migrations": "5.8.6",
    "@mikro-orm/postgresql": "5.8.6",
    "@mikro-orm/reflection": "5.8.6",
    "calendar-date": "2.0.1"
  },
  "devDependencies": {
    "bun-types": "1.0.4",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  }
}
