{
  "name": "node-mikro-orm-playground",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development && ts-node ./src/app.ts",
    "migration:create": "rimraf temp && npx mikro-orm migration:create",
    "migration:up": "npx mikro-orm migration:up"
  },
  "dependencies": {
    "@mikro-orm/cli": "6.2.5",
    "@mikro-orm/migrations":  "6.2.5",
    "@mikro-orm/postgresql": "6.2.5",
    "@mikro-orm/reflection": "6.2.5",
    "calendar-date": "2.0.1"
  },
  "devDependencies": {
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.0.4"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./src/mikro-orm.config.js"
    ]
  }
}
